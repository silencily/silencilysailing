<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="net.silencily.sailing.basic.wf.domain.TblWfStepStatus" table="TBL_WF_STEP_STATUS" >
        <id name="id" type="java.lang.String">
            <column name="ID" length="4000" />
            <generator class="uuid.hex" />
        </id>
        <property name="beanId" type="java.lang.String">
            <column name="BEAN_ID" length="32" />
        </property>
        <property name="stepId" type="java.lang.String">
            <column name="STEP_ID" length="16" />
        </property>
        <property name="nullFieldName" type="java.lang.String">
            <column name="NULL_FIELD_NAME" length="4000" />
        </property>

    </class>
</hibernate-mapping>
